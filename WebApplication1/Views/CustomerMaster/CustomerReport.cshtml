@model List<WebApplication1.Models.ViewModel.CustomerVM>
@{
    ViewBag.Title = "CustomerReport";
}

<h2>CustoemrReport</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model[0].客戶名稱)
        </th>
        <th>
            @Html.DisplayNameFor(model => model[0].聯絡人數量)
        </th>
        <th>
            @Html.DisplayNameFor(model => model[0].銀行帳戶數量)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.客戶名稱)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.聯絡人數量)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.銀行帳戶數量)
            </td>
        </tr>
    }

</table>